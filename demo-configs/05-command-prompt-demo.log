!
! Command Prompt Hostname Demo
! Showcasing various prompt formats with hostnames
!

! User Exec Mode (>) - Limited privileges
Router>
Router>show version
Router>enable

! Privileged Exec Mode (#) - Full access
Router#
Router#show running-config
Router#configure terminal

! Configuration Mode with Context
Router(config)#
Router(config)#hostname CORE-SW-01
CORE-SW-01(config)#

! Interface Configuration Mode
CORE-SW-01(config)#interface GigabitEthernet0/1
CORE-SW-01(config-if)#ip address 192.168.1.1 255.255.255.0
CORE-SW-01(config-if)#description Uplink to Distribution
CORE-SW-01(config-if)#no shutdown
CORE-SW-01(config-if)#exit

! VLAN Configuration Mode
CORE-SW-01(config)#interface Vlan10
CORE-SW-01(config-if)#ip address 10.10.10.1 255.255.255.0
CORE-SW-01(config-if)#exit

! Router Configuration Mode
CORE-SW-01(config)#router bgp 65000
CORE-SW-01(config-router)#bgp router-id 10.0.0.1
CORE-SW-01(config-router)#neighbor 10.0.1.1 remote-as 65000
CORE-SW-01(config-router)#exit

! Line Configuration Mode
CORE-SW-01(config)#line vty 0 4
CORE-SW-01(config-line)#password cisco123
CORE-SW-01(config-line)#login
CORE-SW-01(config-line)#exit

! Various Hostname Formats
EDGE-RTR-01#show ip interface brief
DIST-SW-02#show interfaces status
ACCESS-SW-03#show vlan brief

DATACENTER-CORE-01#show ip bgp summary
BRANCH-RTR-NYC-01#show ip route
CAMPUS-SW-BLDG5-FL2#show mac address-table

! Hostnames with Numbers and Hyphens
SW-3750-STACK-01#show switch
RTR-ASR1K-WAN-01#show platform
FW-ASA-DMZ-01#show conn

